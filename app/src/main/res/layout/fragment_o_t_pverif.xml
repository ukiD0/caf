        app:layout_constraintTop_toBottomOf="@+id/appCompatTextView3" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:background="@drawable/box_gray"
        app:layout_constraintBottom_toBottomOf="@+id/textInputEditText10"
        android:layout_marginLeft="30dp"
        app:layout_constraintStart_toEndOf="@+id/textInputEditText10"
        app:layout_constraintTop_toTopOf="@+id/textInputEditText10" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText7"
